{
  "name": "blog-app",
  "version": "0.1.0",
  "description": "Blog App",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git"
  },
  "scripts": {
    "build:app": "node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/configs/webpack.config.development.js --progress --profile --colors",
    "build:vendor": "node -r babel-register ./node_modules/webpack/bin/webpack --config ./build/configs/webpack.config.vendor.js --progress --profile --colors",
    "hot-server": "node -r babel-register ./build/server/index.js",
    "dev": "concurrently --kill-others \"npm run hot-server\""
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-transform-decorators-legacy": "*",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "browser-sync": "*",
    "clean-webpack-plugin": "*",
    "concurrently": "*",
    "css-loader": "*",
    "eslint": "*",
    "express": "^4.14.0",
    "favicons-webpack-plugin": "*",
    "html-webpack-plugin": "*",
    "json-loader": "*",
    "react-hot-loader": "3.0.0-beta.5",
    "style-loader": "*",
    "webpack": "*",
    "webpack-dev-middleware": "*",
    "webpack-hot-middleware": "*",
    "webpack-merge": "*",
    "webpack-validator": "*"
  },
  "dependencies": {
    "lodash": "*",
    "lowlight": "*",
    "lunr": "*",
    "mobx": "*",
    "mobx-react": "*",
    "oboe": "*",
    "radium": "0.18.1",
    "radium-grid": "2.0.1",
    "react": "*",
    "react-dom": "*",
    "react-syntax-highlighter": "*",
    "react-router": "*",
    "react-tap-event-plugin": "*",
    "react-youtube": "*",
    "rehype": "*",
    "rehype-parse": "*",
    "rehype-react": "*",
    "remark": "*",
    "remark-react": "*",
    "rxjs": "*",
    "unified": "*",
    "whatwg-fetch": "*"
  }
}
